    "def data_2_idx(text, vocab_ids):\n",
    "    idx_text = []\n",
    "    for sentence in text:\n",
    "        indexes = []\n",
    "        for item in sentence:\n",
    "            indexes.append(vocab_ids[item])\n",
    "        idx_text.append(indexes)\n",
    "    return idx_text"
   ]
